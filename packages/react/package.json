{
  "name": "@ds-fancode/redux-rewire-react",
  "version": "0.0.16",
  "description": "State management library for react based applications",
  "keywords": [
    "redux",
    "reducer",
    "state",
    "predictable",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "test": "yarn run -T jest",
    "test-watch": "yarn test -- --watch",
    "test-ci": "yarn test --coverage && coveralls < coverage/lcov.info",
    "lint": "yarn run -T eslint . --cache",
    "lint-fix": "yarn lint --fix",
    "prettier-check": "yarn run -T prettier --check \"**/*.{ts,tsx}\"",
    "prettier-fix": "yarn prettier-check --write \"**/*.{ts,tsx}\"",
    "clean": "rm -rf ./lib ./dist",
    "build": "yarn clean && yarn run -T tsup",
    "prepack": "yarn build",
    "release": "release-it"
  },
  "author": "FanCode",
  "license": "ISC",
  "dependencies": {
    "@ds-fancode/redux-rewire-core": "workspace:*",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.2.6",
    "react": "19.0.0",
    "typescript": "5.7.2"
  },
  "peerDependencies": {
    "react": ">=18.x",
    "redux": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ds-fancode/redux-rewire"
  },
  "sideEffects": false
}
