{
  "name": "@ds-fancode/redux-rewire-core",
  "version": "0.0.16",
  "description": "State management library for react based applications",
  "author": "FanCode",
  "license": "ISC",
  "keywords": [
    "redux",
    "reducer",
    "state",
    "predictable",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "default": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "yarn run -T jest",
    "test-watch": "yarn test -- --watch",
    "test-ci": "yarn test --coverage && coveralls < coverage/lcov.info",
    "lint": "yarn run -T eslint . --cache",
    "lint-fix": "yarn lint --fix",
    "prettier-check": "yarn run -T prettier --check \"**/*.{ts,tsx}\"",
    "prettier-fix": "yarn prettier-check --write \"**/*.{ts,tsx}\"",
    "clean": "rm -rf ./lib ./dist",
    "build": "yarn clean && yarn run -T tsup",
    "prepack": "yarn build",
    "release": "release-it",
    "clean-node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && find . -name 'build' -type d -prune -exec rm -rf '{}' +"
  },
  "dependencies": {
    "mutative": "^1.2.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/jest": "^29.2.4",
    "jsdoc": "^4.0.0",
    "typescript": "5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ds-fancode/redux-rewire"
  },
  "sideEffects": false
}
