{
  "name": "redux-rewire",
  "version": "0.0.16",
  "description": "State management library for react based applications",
  "scripts": {
    "test": "yarn workspaces foreach -Ap run test",
    "test-ci": "yarn workspaces foreach -Ap run test-ci",
    "lint": "yarn workspaces foreach -Ap run lint",
    "lint-fix": "yarn workspaces foreach -Ap run lint-fix",
    "prettier-check": "yarn workspaces foreach -Ap run prettier-check",
    "prettier-fix": "yarn workspaces foreach -Ap run prettier-fix",
    "clean-node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && find . -name 'build' -type d -prune -exec rm -rf '{}' +"
  },
  "author": "FanCode",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@eslint/js": "^8.57.0",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "26.6.3",
    "jsdoc": "^4.0.0",
    "prettier": "^3.2.5",
    "typescript": "5.7.2",
    "typescript-eslint": "^7.3.1"
  },
  "workspaces": [
    "example/*",
    "packages/*",
    "documentation"
  ],
  "resolutions": {
    "typescript": "5.7.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ds-fancode/redux-rewire"
  },
  "packageManager": "yarn@4.6.0"
}
