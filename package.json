{
  "name": "redux-rewire",
  "version": "0.0.15",
  "description": "State management library for react based applications",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest -- --watch",
    "test:ci": "jest --coverage && coveralls < coverage/lcov.info",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint --ext .ts,.tsx --fix",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx}\"",
    "build": "rm -rf ./lib && yarn tsc && yarn tsc --module CommonJS --outDir lib/cjs && node ./publish.js",
    "postinstall": "yarn build"
  },
  "author": "FanCode",
  "license": "ISC",
  "dependencies": {
    "immer": "^9.0.12",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.2.4",
    "chai": "^4.3.7",
    "jest": "26.6.3",
    "jsdoc": "^4.0.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "18.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ds-fancode/redux-rewire"
  }
}
